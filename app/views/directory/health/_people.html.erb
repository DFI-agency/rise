  box-shadow: 1px 5px 20px #ccc;
}

p a {
  line-height: 1.2em;
}

.icon-left {
  margin-right: 5px;
  min-width: 12px;
  font-size: 10px;
  color: #aaa;
}

.masonry-filter-holder .masonry__filters {
  border: none;
  background: #ee6431;
  color: #fff;
}

</style>


