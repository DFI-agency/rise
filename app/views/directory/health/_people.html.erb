   margin-bottom: 0.65em;
}
.label {
  min-width: 0;
  font-size: 8.5px;
}

.boxed.boxed--sm {
    padding: 0;
}
.card__top {
  border-bottom: 1px solid #eee;
  padding: 10px 30px;
  background: #fafafa;
}

.card__top img {

}

.card-1 .card__body {
  padding: .8em 1.4em;
}

.card__bottom {
  padding: 0.5em 1em 1em 1em;
}

.card.card-1 {
